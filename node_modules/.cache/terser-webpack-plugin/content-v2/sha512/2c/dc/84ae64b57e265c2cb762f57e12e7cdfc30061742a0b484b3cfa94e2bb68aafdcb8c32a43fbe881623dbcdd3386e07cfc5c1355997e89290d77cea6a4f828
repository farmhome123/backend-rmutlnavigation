) => {\\r\\n    dispatch(setStateClassToFetching());\\r\\n    await httpClient.delete(`${server.CLASS_URL}/${id}`);\\r\\n    await doGetClass(dispatch);\\r\\n  };\\r\\n};\\r\\n\\r\\nexport const getClass = () => {\\r\\n  return (dispatch) => {\\r\\n    dispatch(setStateClassToFetching());\\r\\n    doGetClass(dispatch);\\r\\n  };\\r\\n};\\r\\n\\r\\nconst doGetClass = (dispatch) => {\\r\\n  httpClient\\r\\n    .get(server.CLASS_URL)\\r\\n    .then((result) => {\\r\\n      dispatch(setStateClassToSuccess(result.data));\\r\\n    })\\r\\n    .catch((error) => {\\r\\n      alert(JSON.stringify(error));\\r\\n      dispatch(setStateClassToFailed());\\r\\n    });\\r\\n};\\r\\n\",\"import React, { Component } from \\\"react\\\";\\r\\nimport * as actions from \\\"./../../actions/class.action\\\";\\r\\nimport { connect } from \\\"react-redux\\\";\\r\\nimport { imageUrl } from \\\"./../../constants\\\";\\r\\nimport { Link } from \\\"react-router-dom\\\";\\r\\nimport _ from \\\"lodash\\\";\\r\\nimport Moment from \\\"react-moment\\\";\\r\\nimport \\\"./class.css\\\";\\r\\nimport Swal from \\\"sweetalert2\\\";\\r\\nimport withReactContent from \\\"sweetalert2-react-content\\\";\\r\\nimport Menusystem from \\\"./../menusystem\\\";\\r\\nimport { getClassByBuildingId } from \\\"./../../actions/class.action\\\";\\r\\nconst MySwal = withReactContent(Swal);\\r\\n\\r\\n\\r\\nclass Class extends Component {\\r\\n\\r\\n  componentDidMount() {\\r\\n    this.debounceSearch = _.debounce(this.props.getClassByKeyword, 500);\\r\\n    // this.props.getClass();\\r\\n    let buildingid = this.props.match.params.id;\\r\\n    let buildingname = this.props.name;\\r\\n    // alert(buildingid)\\r\\n    this.props.getClassByBuildingId(buildingid);\\r\\n  }\\r\\n  createRows = () => {\\r\\n    try {\\r\\n      const { result, isFetching } = this.props.classReducer;\\r\\n      return (\\r\\n        !isFetching &&\\r\\n        result != null &&\\r\\n        result.map((item) => (\\r\\n          <tr key={ite